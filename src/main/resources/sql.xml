<properties>
  <entry key="voucher.insert">
    INSERT INTO vouchers(voucher_id, type, value, created_at)
    VALUES (UUID_TO_BIN(:voucherId), :voucherType, :value, :createdAt)
  </entry>
  <entry key="voucher.update_value">
    UPDATE vouchers SET value = :value
    WHERE voucher_id = UUID_TO_BIN(:voucherId)
  </entry>
  <entry key="voucher.selectAll">
    SELECT * FROM vouchers
  </entry>
  <entry key="voucher.selectById">
    SELECT * FROM vouchers
    WHERE voucher_id = UUID_TO_BIN(:voucherId)
  </entry>
  <entry key="voucher.deleteById">
    DELETE FROM vouchers
    WHERE voucher_id = UUID_TO_BIN(:voucherId)
  </entry>
  <entry key="voucher.deleteAll">
    DELETE FROM vouchers
  </entry>
</properties>